{
    "name": "reactised-fictoan",
    "version": "0.20.0",
    "private": false,
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "buildFolder": "dist",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sujan-s/reactised-fictoan.git"
    },
    "scripts": {
        "start": "rollup -c -w",
        "build": "rollup -c",
        "predeploy": "$npm_execpath run build",
        "deploy": "gh-pages -d dist -e dist && gh-pages -a -d . -s ./package.json",
        "deploy-and-publish": "$npm_execpath run deploy && git checkout gh-pages && $npm_execpath run publish"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@types/lodash-es": "^4.17.3",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.4",
        "@types/react-test-renderer": "^16.9.1",
        "@types/styled-components": "^4.4.1",
        "gh-pages": "^2.0.1",
        "node-sass": "^4.11.0",
        "rollup": "^1.15.6",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^10.0.0",
        "rollup-plugin-copy-glob": "^0.3.0",
        "rollup-plugin-node-resolve": "^5.0.3",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-typescript2": "^0.22.0",
        "rollup-plugin-url": "^2.2.2",
        "tslib": "^1.10.0",
        "typescript": "^3.5.3"
    },
    "files": [
        "dist"
    ],
    "peerDependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.0.1"
    },
    "dependencies": {
        "lodash-es": "^4.17.15",
        "prism-react-renderer": "^1.0.2",
        "styled-components": "^4.4.1",
        "styled-normalize": "^8.0.6"
    }
}
